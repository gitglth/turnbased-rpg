                    target = h1;
                    
                    if(sqrt( (x - h2.x)*(x - h2.x) + 4*(y - h2.y)*(y - h2.y) ) &lt; distmin)
                    {
                        distmin = sqrt( (x - h2.x)*(x - h2.x) + 4*(y - h2.y)*(y - h2.y) );
                        target = h2;
                    }
                    if(sqrt( (x - h3.x)*(x - h3.x) + 4*(y - h3.y)*(y - h3.y) ) &lt; distmin)
                    {
                        distmin = sqrt( (x - h3.x)*(x - h3.x) + 4*(y - h3.y)*(y - h3.y) );
                        target = h3;
                    }
