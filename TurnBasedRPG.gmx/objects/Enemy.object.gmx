                                            path_check_parent(gotoHex);
                                        }
                                    }
                                }
                            }
                            if(ap &gt;= 20)
                            {
                                while(ap &lt; gotoHex.pathDistance * 20)
                                {
                                    gotoHex = gotoHex.pathParent;
                                }
                                instance_create(gotoHex.x, gotoHex.y, MoveMarker);
                                ap -= gotoHex.pathDistance * 20;
                            }
                        }
