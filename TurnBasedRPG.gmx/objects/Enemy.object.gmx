            {
                for(mh = 1; mh &lt;= 7; mh++)
                {
                    moveHex = instance_nearest2(x, y, Hex, mh);
                    if(moveHex.pathMoveMark) break;
                    if(mh == 7)
                    {
                        with(MoveMarker) instance_destroy();
                    }
