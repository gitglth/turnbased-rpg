                                pathDistance = -1;
                                pathParent = -1;
                                pathMoveMark = false;
                            }
                            
                            pathFound = false;
                            gotoHex = hex[hd];
                            if(gotoHex)
                            {
                                if(!gotoHex.occupied)
                                {
                                    h = instance_position(x, y, Hex);
                                    if(h)
